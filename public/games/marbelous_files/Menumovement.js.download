pc.script.create('Menumovement', function (app) {
    // Creates a new Menumovement instance
    var Menumovement = function (entity) {
        this.entity = entity;
    };

    Menumovement.prototype = {
        // Called once after all resources are loaded and before the first update
        initialize: function () {
            this.speed = 0;
        },

        // Called every frame, dt is time in seconds since last update
        update: function (dt) {
            this.speed += 0.1;
            this.entity.setEulerAngles(0,this.speed,0);
        }
    };

    return Menumovement;
});